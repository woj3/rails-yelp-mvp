<div class="container">
  <h1 class="pt-5">Create a new review</h1>
  <h2 class=pb-3>For: <%= @restaurant.name %></h2>

  <%= simple_form_for([@restaurant, @review]) do |f| %>
    <%= f.input :content%>
    <%= f.input :rating %>
    <%= f.submit "Save", class: "btn btn-primary" %>
  <% end %>
</div>
